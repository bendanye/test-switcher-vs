{
	"name": "test-switcher-vs",
	"displayName": "test-switcher-vs",
	"description": "switch to and from test",
	"publisher": "bendanye",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:test-switcher-vs.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "test-switcher-vs.helloWorld",
				"title": "Hello To Tests",
				"category": "Tests"
			}
		],
		"keybindings": [
			{
			  "command": "test-switcher-vs.helloWorld",
			  "key": "Ctrl+9",
			  "mac": "Cmd+9",
			  "when": "editorTextFocus"
			}
		  ],
		  "menus": {
			"editor/context": [
			  {
				"when": "resourceLangId != markdown",
				"command": "test-switcher-vs.helloWorld",
				"group": "navigation"
			  }
			]
		  }
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"repository": {
		"type": "git",
		"url": "https://gitlab.com/bbnn1965/test-switcher-vs.git"
	}
}
